{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "@testing-library/jest-dom": "^5.10.0",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^10.4.1",
    "@types/jest": "^25.2.3",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^13.13.12",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/react-relay": "^7.0.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-router-hash-link": "^1.2.1",
    "@types/relay-runtime": "^8.0.13",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@wasm-tool/wasm-pack-plugin": "^1.3.1",
    "ace-builds": "^1.4.11",
    "babel-plugin-relay": "^9.1.0",
    "clsx": "^1.1.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "gdlk_wasm": "file:../wasm/pkg",
    "graphql": "^15.1.0",
    "graphql-schema-linter": "^0.4.0",
    "http-proxy-middleware": "^1.0.4",
    "lodash-es": "^4.17.15",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-ace": "^8.1.0",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^16.13.1",
    "react-relay": "^9.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "relay-compiler": "^9.1.0",
    "relay-compiler-language-typescript": "^12.0.3",
    "relay-config": "^9.1.0",
    "relay-hooks": "^3.5.0",
    "source-map-explorer": "^2.4.2",
    "typescript": "^3.9.5",
    "wasm-loader": "^1.3.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "wasm-pack": "cd ../wasm; wasm-pack build",
    "analyze": "mkdir -p analysis && source-map-explorer 'build/static/js/*.js' --html > analysis/index.html",
    "clean": "rm -rf node_modules **/__generated__",
    "lint": "eslint './**/*.{js,ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "lint:gql": "graphql-schema-linter",
    "relay": "relay-compiler",
    "tsc": "tsc"
  },
  "graphql-schema-linter": {
    "schemaPaths": [
      "../api/schema.graphql"
    ],
    "rules": [
      "arguments-have-descriptions",
      "defined-types-are-used",
      "deprecations-have-a-reason",
      "descriptions-are-capitalized",
      "enum-values-all-caps",
      "enum-values-have-descriptions",
      "fields-are-camel-cased",
      "fields-have-descriptions",
      "input-object-values-have-descriptions",
      "relay-connection-types-spec",
      "relay-connection-arguments-spec",
      "types-are-capitalized",
      "types-have-descriptions"
    ]
  }
}
